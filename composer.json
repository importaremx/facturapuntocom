{
    "name": "importaremx/facturapuntocom",
    "description": "Api Client para la api de factura.com",
    "license": "GPL-1.0-or-later",
    "authors": [
        {
            "name": "Geovanni Simuta",
            "email": "geovanni@importare.mx"
        }
    ],
    "homepage": "https://github.com/importaremx/facturapuntocom",
    "keywords": ["Laravel", "Facturapuntocom","factura.com"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Importaremx\\Facturapuntocom\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Importaremx\\Facturapuntocom\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Importaremx\\Facturapuntocom\\FacturapuntocomServiceProvider"
            ],
            "aliases": {
                "Facturapuntocom": "Importaremx\\Facturapuntocom\\Facades\\Facturapuntocom"
            }
        }
    }
}
